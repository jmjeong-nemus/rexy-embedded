#
ifeq "$(CFG)" "Debug"
OUTDIR=Debug
OUTFILE=$(OUTDIR)/rexye
CFG_INC=-I../../../../core/header/ -I../../../../logic/header/ \
	-I../../ -I../../../../resource/ -I/usr/include/gtk-2.0/gtk/ \
	-I/usr/include/gtk-2.0/gdk/ -I/usr/include/gtk-2.0/ \
	-I/usr/lib/gtk-2.0/include/ -I/usr/include/atk-1.0/ \
	-I/usr/include/cairo/ -I/usr/include/pango-1.0/ \
	-I/usr/include/glib-2.0/ -I/usr/lib/glib-2.0/include/ 
CFG_LIB=
CFG_OBJ=
COMMON_OBJ=$(OUTDIR)/rexy_clib.o $(OUTDIR)/rexy_halinterface.o \
	$(OUTDIR)/startup.o 
OBJ=$(COMMON_OBJ) $(CFG_OBJ)
ALL_OBJ=$(OUTDIR)/rexy_clib.o $(OUTDIR)/rexy_halinterface.o \
	$(OUTDIR)/startup.o 

COMPILE=gcc -c   -g -ggdb -o "$(OUTDIR)/$(*F).o" $(CFG_INC) "$<"
LINK=gcc  -g -ggdb -o "$(OUTFILE)" $(ALL_OBJ)

# Pattern rules
$(OUTDIR)/%.o : ../../device/%.c
	$(COMPILE)

# Build rules
all: $(OUTFILE)

$(OUTFILE): $(OUTDIR)  $(OBJ)
	$(LINK)

$(OUTDIR):
	mkdir -p "$(OUTDIR)"

# Rebuild this project
rebuild: cleanall all

# Clean this project
clean:
	rm -f $(OUTFILE)
	rm -f $(OBJ)

# Clean this project and all dependencies
cleanall: clean
endif

