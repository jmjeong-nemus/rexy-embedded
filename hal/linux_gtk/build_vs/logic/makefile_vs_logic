# Make command to use for dependencies
MAKE=make

# If no configuration is specified, "Debug" will be used
ifndef "CFG"
CFG=Debug
endif

#
# Configuration: Debug
#
ifeq "$(CFG)" "Debug"
OUTDIR=Debug
OUTFILE=logic.a
CFG_INC=-I../../../../core/header/ -I../../ \
	-I../../../../logic/header/ -I../../../../resource/ \
	-I../../../../control/header/ -I../../../../application/ 
CFG_LIB=
CFG_OBJ=
COMMON_OBJ=$(OUTDIR)/rexy_ctl_core.o $(OUTDIR)/rexy_main.o \
	$(OUTDIR)/rexy_mainproc.o $(OUTDIR)/rexy_pict.o \
	$(OUTDIR)/rexy_state.o $(OUTDIR)/rexy_sysmsg.o 
OBJ=$(COMMON_OBJ) $(CFG_OBJ)
ALL_OBJ=$(OUTDIR)/rexy_ctl_core.o $(OUTDIR)/rexy_main.o \
	$(OUTDIR)/rexy_mainproc.o $(OUTDIR)/rexy_pict.o \
	$(OUTDIR)/rexy_state.o $(OUTDIR)/rexy_sysmsg.o 

COMPILE=gcc -c   -g -ggdb -o "$(OUTDIR)/$(*F).o" $(CFG_INC) "$<"
LINK=ar -rs  "$(OUTFILE)" $(OBJ)

# Pattern rules
$(OUTDIR)/%.o : ../../../../logic/source/%.c
	$(COMPILE)

# Build rules
all: $(OUTFILE)

$(OUTFILE): $(OUTDIR)  $(OBJ)
	$(LINK)

$(OUTDIR):
	mkdir -p "$(OUTDIR)"

# Rebuild this project
rebuild: cleanall all

# Clean this project
clean:
	rm -f $(OUTFILE)
	rm -f $(OBJ)

# Clean this project and all dependencies
cleanall: clean
endif

